"%(divider)s\n"
"%(traceback)s\n"
"%(exception)s\n"
"%(divider)s"
msgstr ""
"Tapahtui virhe:\n"
"%(divider)s\n"
"%(traceback)s\n"
"%(exception)s\n"
"%(divider)s"
